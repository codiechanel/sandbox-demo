{
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
          "jsx": true
      }

    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "off",
      "react/jsx-uses-vars": 1
      
    }
  }